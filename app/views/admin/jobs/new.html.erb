<div class="col-md-9">
  <div class="panel panel-info">
    <div class="panel-heading"><h4>Add a job<h4></div>
    <div class="panel-body">
      <%= simple_form_for [:admin,@job] do |f| %>

      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :city %>
      <%= f.input :wage_lower_bound, :label => "薪资下限" %>
      <%= f.input :wage_upper_bound, :label => "薪资上限" %>
      <%= f.input :contact_email %>
      <%= f.input :is_hidden %>

        <%= f.submit "Submit",class:"btn btn-danger"%>
      <% end %>
    </div>
  </div>
</div>
